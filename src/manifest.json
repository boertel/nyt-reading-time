{
    "manifest_version": 2,
    "name": "nyt reading time",
    "version": "0.2",
    "content_scripts": [
        {
            "matches": [
                "https://www.nytimes.com/*",
                "https://www.theguardian.com/*"
            ],
            "js": [
                "nyt-reading-time.js"
            ],
            "run_at": "document_end"
        }
    ]
}

